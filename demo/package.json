{
  "name": "dicom-web-anonymizer",
  "version": "0.1.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openradx/dicom-anonymizer.git"
  },
  "keywords": [
    "DICOM",
    "De-identification",
    "Pseudonymization",
    "Radiology"
  ],
  "author": "Luca Dulz",
  "description": "A DICOM web pseudonymization library in JavaScript.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openradx/dicom-anonymizer/issues"
  },
  "homepage": "https://github.com/openradx/dicom-anonymizer#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "tsup": "^8.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.3"
  },
  "dependencies": {
    "dcmjs": "^0.30.1",
    "fs": "^0.0.1-security",
    "get-random-values": "^3.0.0"
  },
  "files": [
    "dist",
    "types/dcm.d.ts"
  ],
  "main": "./dist/dicom-web-anonymizer.umd.cjs",
  "module": "./dist/dicom-web-anonymizer.js",
  "types": "./**/*.d.ts",
  "exports": {
    ".": {
      "import": "./dist/dicom-web-anonymizer.js",
      "require": "./dist/dicom-web-anonymizer.umd.cjs"
    }
  }
}
